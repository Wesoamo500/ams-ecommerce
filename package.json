{
  "name": "@ams-ecommerce/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "db:dev:rm": "docker-compose rm dev-db -s -f -v",
    "db:dev:up": "docker-compose up dev-db -d",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up",
    "seed": "ts-node apps/server/src/typeorm/seeds/main.seed.ts",
    "db:test:rm": "docker compose rm dev-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && yarn prisma:test:deploy",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --watchAll --no-cache --config ./test/jest-e2e.json"
  },
  "private": true,
  "dependencies": {
